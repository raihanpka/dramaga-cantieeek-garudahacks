# ðŸš€ NusaScan Environment Configuration
# Copy this file to .env and fill in your API keys

# ===== REQUIRED API KEYS =====

# OpenAI API Key (Required for AI Agent)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Google Search API (Optional - for cultural search)
# Get from: https://console.developers.google.com/
GOOGLE_SEARCH_API_KEY=your_google_search_api_key
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id
# Get from: https://huggingface.co/settings/tokens
HF_API_TOKEN=hf_your-huggingface-token-here

# ===== OPTIONAL SEARCH APIs =====

# Google Custom Search (Optional - for enhanced grounding search)
# Get from: https://developers.google.com/custom-search/v1/introduction
GOOGLE_SEARCH_API_KEY=your-google-search-api-key-here
GOOGLE_SEARCH_ENGINE_ID=your-custom-search-engine-id-here

# ===== SERVER CONFIGURATION =====

# Server Port (default: 3000)
PORT=3000

# Environment (development/production)
NODE_ENV=development

# File Upload Settings
MAX_FILE_SIZE=10485760
UPLOAD_DIR=./uploads

# Performance Settings
ANALYSIS_TIMEOUT=15000
MAX_CONCURRENT_ANALYSES=5

# ===== CULTURAL HERITAGE SETTINGS =====

# Default language for OCR
DEFAULT_OCR_LANGUAGE=eng+ind+jav

# Supported cultural categories
CULTURAL_CATEGORIES=candi,wayang,batik,keris,naskah

# Regional focus areas
SUPPORTED_REGIONS=aceh,sumatra-utara,sumatra-barat,riau,kepulauan-riau,jambi,sumatra-selatan,bengkulu,lampung,bangka-belitung,dki-jakarta,jawa-barat,jawa-tengah,di-yogyakarta,jawa-timur,banten,bali,nusa-tenggara-barat,nusa-tenggara-timur,kalimantan-barat,kalimantan-tengah,kalimantan-selatan,kalimantan-timur,kalimantan-utara,sulawesi-utara,sulawesi-tengah,sulawesi-selatan,sulawesi-tenggara,gorontalo,sulawesi-barat,maluku,maluku-utara,papua-barat,papua,papua-pegunungan,papua-tengah,papua-selatan,papua-barat-daya

# ===== LOGGING & MONITORING =====

# Log level (error, warn, info, debug)
LOG_LEVEL=info

# Enable performance monitoring
ENABLE_METRICS=true

# ===== DATA STORAGE =====
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
TURSO_DB_URL=file:mastra.db
TURSO_AUTH_TOKEN=
MEMORY_TYPE=hybrid