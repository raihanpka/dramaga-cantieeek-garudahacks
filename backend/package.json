{
  "name": "kalanusa-backend",
  "version": "1.0.0",
  "description": "Real-time AI system for Indonesian cultural heritage analysis",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx server.ts",
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.prod.json && cp -r database dist/ 2>/dev/null || true",
    "build:bun": "bun build server.ts --outdir ./dist --target node",
    "dev:bun": "bun run server.ts",
    "start:bun": "NODE_ENV=production bun run server.ts",
    "test:scan": "tsx testing/testScan.ts",
    "test:chat": "tsx testing/testChat.ts",
    "test:api": "tsx testing/testAPIs.ts",
    "start-realtime": "tsx start-realtime.ts",
    "monitor": "tsx start-realtime.ts",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ai-sdk/google-vertex": "^2.2.27",
    "@ai-sdk/openai": "^1.3.23",
    "@mastra/core": "^0.11.1",
    "@mastra/evals": "^0.10.7",
    "@mastra/fastembed": "^0.10.1",
    "@mastra/libsql": "^0.11.2",
    "@mastra/memory": "^0.11.5",
    "@mastra/qdrant": "^0.11.1",
    "@qdrant/js-client-rest": "^1.15.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "mastra": "^0.10.15",
    "multer": "^2.0.2",
    "openai": "^5.10.2",
    "tesseract.js": "^6.0.1"
  }
}
